else
  export NODE_PATH="$NODE_PATH:/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/json-refs@3.0.15/node_modules/json-refs/bin/node_modules:/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/json-refs@3.0.15/node_modules/json-refs/node_modules:/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/json-refs@3.0.15/node_modules:/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/node_modules:/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules:/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedittools/common/node_modules:/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedittools/node_modules:/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/node_modules:/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/node_modules:/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/node_modules:/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/node_modules:/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/node_modules:/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/node_modules:/home/wadmin/Documents/node_modules:/home/wadmin/node_modules:/home/node_modules:/node_modules"
