{"code":"import { createCustomElement } from '@angular/elements';\r\nimport { functionsUtils } from '@smart/utils';\r\nimport { diNameUtils } from './DINameUtils';\r\n/**\r\n * Class level typescript {@link http://www.typescriptlang.org/docs/handbook/decorators.html decorator factory}\r\n * used to declare a Smartedit custom component from a Depencency injection standpoint.\r\n */\r\nexport const SeCustomComponent = function () {\r\n    return function (componentConstructor) {\r\n        const componentName = functionsUtils.getConstructorName(componentConstructor);\r\n        /* forbiddenNameSpaces window._:false */\r\n        const definition = window.__smartedit__.getDecoratorPayload('Component', componentConstructor);\r\n        if (!definition) {\r\n            throw new Error(`@SeCustomComponent ${componentName} should only be used on a @Component decorated class`);\r\n        }\r\n        const selector = parseComponentSelector(definition.selector, componentConstructor);\r\n        componentConstructor.selector = selector;\r\n        seCustomComponents.unshift(componentConstructor);\r\n        return componentConstructor;\r\n    };\r\n};\r\nexport const parseComponentSelector = function (selector, seContructor) {\r\n    if (!selector) {\r\n        return window.smarteditLodash.kebabCase(diNameUtils.buildComponentName(seContructor));\r\n    }\r\n    else {\r\n        return selector;\r\n    }\r\n};\r\nexport const seCustomComponents = [];\r\nexport function registerCustomComponents(injector) {\r\n    // create a custom element for every @SeComponent flagged with custom:true\r\n    seCustomComponents.forEach((myConstructor) => {\r\n        if (!customElements.get(myConstructor.selector)) {\r\n            // Convert to a custom element.\r\n            const customComponent = createCustomElement(myConstructor, { injector });\r\n            // Register the custom element with the browser.\r\n            customElements.define(myConstructor.selector, customComponent);\r\n        }\r\n    });\r\n}\r\n//# sourceMappingURL=SeCustomComponent.js.map","references":["/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/@angular+core@8.2.14_rxjs@6.5.4+zone.js@0.9.1/node_modules/@angular/core/core.d.ts","/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/@angular+elements@8.2.14_1aef5024cf23ac83cf007252fbe55f5a/node_modules/@angular/elements/elements.d.ts","/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedit/apps/smart-utils/dist/index.d.ts","/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedit/apps/smartedit-commons/src/di/DINameUtils.ts","/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedit/apps/smartedit-commons/src/di/types.ts"],"map":"{\"version\":3,\"file\":\"SeCustomComponent.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../src/di/SeCustomComponent.ts\"],\"names\":[],\"mappings\":\"AAIA,OAAO,EAAE,mBAAmB,EAAE,MAAM,mBAAmB,CAAC;AACxD,OAAO,EAAE,cAAc,EAAE,MAAM,cAAc,CAAC;AAC9C,OAAO,EAAE,WAAW,EAAE,MAAM,eAAe,CAAC;AAG5C;;;GAGG;AACH,MAAM,CAAC,MAAM,iBAAiB,GAAG;IAC7B,OAAO,UAA4C,oBAAuB;QACtE,MAAM,aAAa,GAAG,cAAc,CAAC,kBAAkB,CAAC,oBAAoB,CAAC,CAAC;QAE9E,wCAAwC;QACxC,MAAM,UAAU,GAAG,MAAM,CAAC,aAAa,CAAC,mBAAmB,CACvD,WAAW,EACX,oBAAoB,CACvB,CAAC;QAEF,IAAI,CAAC,UAAU,EAAE;YACb,MAAM,IAAI,KAAK,CACX,sBAAsB,aAAa,sDAAsD,CAC5F,CAAC;SACL;QAED,MAAM,QAAQ,GAAG,sBAAsB,CAAC,UAAU,CAAC,QAAQ,EAAE,oBAAoB,CAAC,CAAC;QACnF,oBAAoB,CAAC,QAAQ,GAAG,QAAQ,CAAC;QAEzC,kBAAkB,CAAC,OAAO,CAAC,oBAAoB,CAAC,CAAC;QAEjD,OAAO,oBAAoB,CAAC;IAChC,CAAC,CAAC;AACN,CAAC,CAAC;AAEF,MAAM,CAAC,MAAM,sBAAsB,GAAG,UAClC,QAAgB,EAChB,YAAoC;IAEpC,IAAI,CAAC,QAAQ,EAAE;QACX,OAAO,MAAM,CAAC,eAAe,CAAC,SAAS,CAAC,WAAW,C