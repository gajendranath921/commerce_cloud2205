storageService\r\n                .getValueFromLocalStorage('SELECTED_LANGUAGE', false)\r\n                .then((lang) => lang ? lang.isoCode : browserService.getBrowserLocale(), () => browserService.getBrowserLocale())\r\n                .then((lang) => {\r\n                $translate.use(lang);\r\n            });\r\n            operationContextService.register(I18N_RESOURCE_URI, OPERATION_CONTEXT.TOOLING);\r\n            crossFrameEventService.subscribe(SWITCH_LANGUAGE_EVENT, (id, data) => {\r\n                $translate.use(data.isoCode);\r\n            });\r\n        }\r\n    })\r\n], /* @ngInject */ TranslationServiceModule);\r\nexport { /* @ngInject */ TranslationServiceModule };\r\n//# sourceMappingURL=translationServiceModule.js.map","references":["/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/@types+angular-translate@2.16.2/node_modules/@types/angular-translate/index.d.ts","/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedit/apps/smart-utils/dist/index.d.ts","/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedit/apps/smartedit-commons/src/di/index.ts","/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedit/apps/smartedit-commons/src/utils/smarteditconstants.ts","/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedit/apps/smartedit-commons/src/services/index.ts","/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedit/apps/smartedit-commons/src/modules/translations/translateStaticFilesLoader.ts"],"map":"{\"version\":3,\"file\":\"translationServiceModule.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../src/modules/translations/translatio