{"code":"import { __decorate } from \"tslib\";\r\n/*\r\n * Copyright (c) 2022 SAP SE or an SAP affiliate company. All rights reserved.\r\n */\r\nimport { Pipe } from '@angular/core';\r\n/**\r\n * Pipe used to filter array of objects by object passed as an argument.\r\n *\r\n * The pipe will return array of objects that contains the exact keys and values of passed object.\r\n *\r\n * ### Example\r\n *\r\n *      <div *ngFor='let item of items | seProperty:{ isEnabled: true }'></div>\r\n */\r\nlet PropertyPipe = class PropertyPipe {\r\n    transform(array, propObject) {\r\n        if (!array) {\r\n            return undefined;\r\n        }\r\n        if (!propObject) {\r\n            return [...array];\r\n        }\r\n        return array.filter((item) => Object.keys(propObject).every((key) => item[key] === propObject[key]));\r\n    }\r\n};\r\nPropertyPipe = __decorate([\r\n    Pipe({ name: 'seProperty' })\r\n], PropertyPipe);\r\nexport { PropertyPipe };\r\n//# sourceMappingURL=PropertyPipe.js.map","references":["/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedittools/common/temp/node_modules/.pnpm/@angular+core@8.2.14_rxjs@6.5.4+zone.js@0.9.1/node_modules/@angular/core/core.d.ts","/home/wadmin/Documents/cloud-commerce-sample-setup-update-2211/core-customize/hybris/bin/modules/smartedit/smartedit/apps/smart-utils/dist/index.d.ts"],"map":"{\"version\":3,\"file\":\"PropertyPipe.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../src/pipes/property/PropertyPipe.ts\"],\"names\":[],\"mappings\":\";AAAA;;GAEG;AACH,OAAO,EAAE,IAAI,EAAiB,MAAM,eAAe,CAAC;AAGpD;;;;;;;;GAQG;AAEH,IAAa,YAAY,GAAzB,MAAa,YAAY;IACrB,SAAS,CAAC,KAAU,EAAE,UAA4B;QAC9C,IAAI,CAAC,KAAK,EAAE;YACR,OAAO,SAAS,CAAC;SACpB;QAED,IAAI,CAAC,UAAU,EAAE;YACb,OAAO,CAAC,GAAG,KAAK,CAAC,CAAC;SACrB;QAED,OAAO,KAAK,CAAC,MAAM,CAAC,CAAC,IAAI,EAAE,EAAE,CACzB,MAAM,CAAC,IAAI,CAAC,UAAU,CAAC,CAAC,KAAK,CAAC,CAAC,GAAW,EAAE,EAAE,CAAC,IAAI,CAAC,GAAG,CAAC,KAAK,UAAU,CAAC,